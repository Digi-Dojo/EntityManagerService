<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">
    <class>it.unibz.digidojo.entitymanagerservice.user.domain.User</class>
    <class>it.unibz.digidojo.entitymanagerservice.teammember.domain.TeamMember</class>
    <class>it.unibz.digidojo.entitymanagerservice.startup.domain.Startup</class>
    <properties>
      <property
            name="hibernate.globally_quoted_identifiers"
            value="true"
      />
      <property
            name="hibernate.globally_quoted_identifiers_skip_column_definitions"
            value="true"
      />
  </properties>
  </persistence-unit>

</persistence>
